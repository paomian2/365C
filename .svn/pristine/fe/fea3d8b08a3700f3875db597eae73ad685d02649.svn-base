<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background">

   <include layout="@layout/layout_msg_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="vertical"
        android:background="@color/white">
        <LinearLayout
            android:id="@+id/rl_person_info_head"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="头像"
                android:textSize="14sp"
                android:textColor="@color/black"/>
            <com.commsdk.common.widget.CircleImageView
                android:id="@+id/img_person_user_head"
                android:layout_width="82px"
                android:layout_height="82px"
                android:layout_marginRight="18px"
                android:scaleType="centerCrop"
                android:src="@mipmap/myself_head_portrait"/>
            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>

        <LinearLayout
            android:id="@+id/rl_person_info_nickname"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="昵称"
                android:textSize="14sp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/tv_personal_info_nikename"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="18px"
                android:textSize="14sp"
                android:textColor="@color/corlor_666"/>
            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>

        <LinearLayout
            android:id="@+id/rl_person_info_phone"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="手机号"
                android:textSize="14sp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/tv_personal_info_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="50px"
                android:text="13236548563"
                android:textSize="14sp"
                android:textColor="@color/corlor_666"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>
        <LinearLayout
            android:id="@+id/rl_person_info_weixin"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="微信"
                android:textSize="14sp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/tv_personal_info_weixin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="18px"
                android:text="13236548563"
                android:textSize="14sp"
                android:textColor="@color/corlor_666"/>
            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>

        <LinearLayout
            android:id="@+id/rl_person_info_blankcard"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="我的银行卡"
                android:textSize="14sp"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>

        <LinearLayout
            android:id="@+id/rl_person_info_realname"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="实名认证"
                android:textSize="14sp"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_back"/>

        <LinearLayout
            android:id="@+id/rl_person_info_Invitationcode"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:background="@color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:layout_marginLeft="20px"
                android:text="补填邀请码"
                android:textSize="14sp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/tv_personal_info_Invitationcode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="18px"
                android:text="13236548563"
                android:textSize="14sp"
                android:textColor="@color/corlor_666"/>
            <ImageView
                android:layout_width="12px"
                android:layout_height="22px"
                android:src="@mipmap/right_arrow"
                android:layout_marginRight="20px"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
