public class BalanceBean {

    /**整体余额*/
    private String totalBalance="0";
    /**平台余额*/
    private String platformBalance="0";
    /**充值余额*/
    private String rechargeBalance="0";

    public String getTotalBalance() {
        return totalBalance;
    }

    public void setTotalBalance(String totalBalance) {
        this.totalBalance = totalBalance;
    }

    public String getPlatformBalance() {
        return platformBalance;
    }

    public void setPlatformBalance(String platformBalance) {
        this.platformBalance = platformBalance;
    }

    public String getRechargeBalance() {
        return rechargeBalance;
    }

    public void setRechargeBalance(String rechargeBalance) {
        this.rechargeBalance = rechargeBalance;
    }
}
