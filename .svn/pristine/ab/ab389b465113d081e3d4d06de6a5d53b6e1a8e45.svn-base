<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="160px">
       <com.commsdk.common.widget.CircleImageView
           android:layout_marginLeft="36px"
           android:layout_marginTop="43px"
           android:id="@+id/headImg"
           android:layout_width="80px"
           android:layout_height="80px"
           android:src="@mipmap/myself_head_portrait"/>

       <TextView
           android:layout_centerVertical="true"
           android:id="@+id/tvStoreName"
           android:layout_marginLeft="28px"
           android:layout_toRightOf="@+id/headImg"
           android:text="樱桃小丸子"
           android:textSize="28px"
           android:textColor="@color/tv_hint"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content" />

       <TextView
           android:drawablePadding="20px"
           android:drawableRight="@mipmap/right_arrow"
           android:layout_centerVertical="true"
           android:layout_marginRight="46px"
           android:layout_alignParentRight="true"
           android:id="@+id/tvBalancePrice"
           android:text="+12"
           android:textSize="30px"
           android:textColor="@color/black"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content" />

       <View
           android:layout_width="match_parent"
           android:layout_height="1px"
           android:layout_alignParentBottom="true"
           android:layout_marginLeft="40px"
           android:background="@color/line_back" />
   </RelativeLayout>



</RelativeLayout>
