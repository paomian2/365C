<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--注册页面EditText-->
    <style name="edt_register">
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">@dimen/font_common_size_medium</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <!--密码输入框-->
    <style name="edt_pwd_input">
        <item name="android:singleLine">true</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">@dimen/font_common_size_medium</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">16</item>
        <item name="android:digits">0123456789abcdefghigklmnopqrstuvwxyzABCDEFGHIGKLMNOPQRSTUVWXYZ
        </item>
    </style>

    <!--注册页面第二頁Relayout-->
    <style name="layout_register">
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <!--注册页面第二頁TextView-->
    <style name="tv_regiester2">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:drawableLeft">@drawable/icon_not_emptey</item>
        <item name="android:drawablePadding">0dp</item>
    </style>

    <!--注册页面第二頁TextView,不带星号-->
    <style name="tv_regiester2_1">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:drawablePadding">0dp</item>
    </style>


    <!--注册页面第二頁TextView-->
    <style name="edt_regiester2">
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/edt_bg</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
    </style>

    <!--公用的RelayoutLayout  Item -->
    <style name="relayout_common_style">
        <item name="android:layout_marginTop">1px</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_height">80px</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <!--帶drawable的TextView,放在relayout_common_style裏面，左侧-->
    <style name="tv_left_common">
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
    </style>

    <!--帶drawable的TextView,放在relayout_common_style裏面，右侧-->
    <style name="tv_right_common">
        <item name="android:textColor">@color/tv_hint</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:drawableRight">@drawable/icon_next</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
    </style>


    <!--checkBox,Radio专用-->
    <style name="cb_commom_style">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:background">@null</item>
        <item name="android:drawableLeft">@drawable/selector_radio_button_regiser</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">11dp</item>
    </style>

    <!--登录页面layout布局-->
    <style name="layout_login">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
    </style>

    <!--登录页面EditText-->
    <style name="edt_login">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textSize">@dimen/font_common_size_medium</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
    </style>


    <style name="mine_text_top">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="mine_text_item">
        <item name="android:drawableRight">@drawable/icon_home_index_def</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
    </style>

    <style name="edt_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:background">@drawable/edt_bg</item>
        <item name="android:padding">10dp</item>

    </style>

    <style name="text_setting">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_marginTop">1px</item>
        <item name="android:background">@color/white</item>
        <item name="android:drawableRight">@drawable/icon_next</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="text_information">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/tv_hint</item>
        <item name="android:textSize">@dimen/font_common_size_small</item>
        <item name="android:gravity">right|center</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="txt_single">
        <item name="android:layout_marginTop">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80px</item>
        <item name="android:paddingLeft">20px</item>
        <item name="android:textSize">28px</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="txt_single2" parent="@style/txt_single">
        <item name="android:textColor">#666666</item>
    </style>


</resources>
