<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="36dp">

    <TextView
        android:visibility="gone"
        android:layout_centerHorizontal="true"
        android:text="0"
        android:id="@+id/tvCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@+id/tvCount"
        android:id="@+id/personal_item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="集市" />

    <ImageView
        android:id="@+id/personal_item_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_home_index_def"
        android:layout_above="@+id/personal_item_title"
        android:layout_toRightOf="@+id/personal_item_title"
        android:layout_marginLeft="-2dp"
        android:layout_marginBottom="-8dp"
        android:visibility="gone"/>
</RelativeLayout>
