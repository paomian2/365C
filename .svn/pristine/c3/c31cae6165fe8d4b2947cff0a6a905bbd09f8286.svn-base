<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30px"
        android:layout_marginTop="46px"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_integral_detail_source"
            android:layout_width="match_parent"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:text="订单抵扣"
            android:textSize="15sp"
            android:textColor="@color/corlor_666"/>

        <TextView
            android:id="@+id/tv_integral_detail_item_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="54px"
            android:text="78"
            android:textSize="15sp"
            android:textColor="@color/corlor_666"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30px"
        android:layout_marginTop="10px"
        android:layout_marginBottom="20px"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_integral_detail_orderNo"
            android:layout_width="match_parent"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:text="订单编号"
            android:textSize="12sp"
            android:textColor="@color/corlor_666"/>

        <TextView
            android:id="@+id/tv_integral_detail_orderTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="46px"
            android:text="07-12"
            android:textSize="15sp"
            android:textColor="@color/tv_hint"/>
    </LinearLayout>

</LinearLayout>
