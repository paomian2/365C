<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e0000000">
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="658px"
            android:layout_height="816px"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/pw_background_white">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <com.commsdk.common.widget.CircleImageView
                    android:id="@+id/img_my_head"
                    android:layout_width="120px"
                    android:layout_height="120px"
                    android:layout_marginTop="20px"
                    android:layout_marginLeft="20px"
                    android:src="@mipmap/myself_head_portrait"/>
                <TextView
                    android:id="@+id/tv_my_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="42px"
                    android:layout_marginLeft="30px"
                    android:text="wowo"
                    android:textSize="15sp"
                    android:textColor="@color/black"/>

            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="87px"
                android:gravity="center_horizontal"
                android:text="您还没有达到分销级别呦~"
                android:textSize="18sp"
                android:textColor="@color/black"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="60px"
                android:layout_marginLeft="56px"
                android:text="您可以通过以下方式成为分销达人"
                android:textSize="12sp"
                android:textColor="@color/corlor_666"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="48px"
                android:layout_marginLeft="56px"
                android:lineSpacingMultiplier="1.5"
                android:text="@string/distribution_requirement"
                android:textSize="15sp"
                android:textColor="@color/black"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="106px"
                android:layout_marginLeft="412px"
                android:text="加油呦~"
                android:textSize="12sp"
                android:textColor="@color/corlor_666"/>
        </LinearLayout>

        <ImageView
            android:id="@+id/img_canclePw"
            android:layout_width="80px"
            android:layout_height="80px"
            android:layout_marginTop="76px"
            android:layout_gravity="center_horizontal"
            android:src="@mipmap/canclepw"/>
    </LinearLayout>

</RelativeLayout>
